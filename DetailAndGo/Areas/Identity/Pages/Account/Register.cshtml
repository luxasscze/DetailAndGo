<script>
    function checkEmailExists() {
        var email = $("#emailInput").val();
        
        $.ajax({
            url: "?handler=CheckEmailExists",
            data: { email: email },
            type: "GET",
            async: false,
            beforeSend: function () {
                
            },
            success: function (data) {
                if(data == true) {
                    document.getElementById('emailExists').innerHTML = "E-Mail already taken...";
                    $('#nextBtn').attr('disabled','disabled');
                }
                else {
                    document.getElementById('emailExists').innerHTML = "";
                    $('#nextBtn').removeAttr('disabled');
                }
            },
            error: function () {
                console.log("Error");
            }
        });
    }       
    

    $('#emailInput').on('focusout', function () {
        checkEmailExists($('#emailInput').val());
    });

    var input = document.getElementById('autocomplete');
      var autocomplete = new google.maps.places.Autocomplete(input);

    autocomplete.addListener('place_changed', function () {

        var place = autocomplete.getPlace();

        // place variable will have all the information you are looking for.

        console.log(place);
        $('#inputAddress1').val(place.address_components[1].long_name);
        $('#inputAddress2').val(place.address_components[2].long_name);
        $('#inputAddress3').val(place.address_components[3].long_name);
        if(place.address_components.length == 7) {
        $('#inputPostCode').val(place.address_components[0].long_name);
        }
        else {
            $('#inputPostCode').val(place.address_components[7].long_name);
        }

    });
</script>

